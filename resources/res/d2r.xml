<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="?android:attr/colorControlHighlight">
    <item android:id="@android:id/mask">
        <shape>
            <corners android:radius="8dp"/>
            <solid android:color="?attr/(name removed)"/>
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <corners android:radius="8dp"/>
                    <solid android:color="@color/(name removed)"/>
                </shape>
            </item>
            <item>
                <shape>
                    <corners android:radius="8dp"/>
                    <solid android:color="?attr/(name removed)"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>
