<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <LinearLayout android:orientation="horizontal" android:id="@+id/wa_switch_view_container" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:layout_gravity="center_vertical" android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="30dp" android:layout_weight="1">
            <com.whatsapp.WaTextView android:id="@+id/switch_view_title" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.whatsapp.WaTextView android:id="@+id/switch_view_description" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="false" style="@style/(name removed)"/>
        </LinearLayout>
        <com.whatsapp.wds.components.toggle.WDSSwitch android:id="@+id/switch_view_toggle" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </LinearLayout>
</merge>
