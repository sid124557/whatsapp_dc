<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <size android:height="64dp" android:width="64dp"/>
            <solid android:color="@color/(name removed)"/>
        </shape>
    </item>
    <item android:left="14dp" android:top="14dp" android:right="14dp" android:bottom="14dp">
        <bitmap android:gravity="fill" android:src="@drawable/pix_logo" android:tint="@color/(name removed)"/>
    </item>
</layer-list>
