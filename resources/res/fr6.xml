<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="@dimen/(name removed)">
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.whatsapp.components.button.ThumbnailButton android:id="@+id/contact_photo" style="@style/(name removed)"/>
        <ViewStub android:id="@+id/subgroup_photo" android:visibility="gone" android:layout="@layout/(name removed)" android:inflatedId="@+id/subgroup_photo" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/(name removed)"/>
        <RelativeLayout android:gravity="center_vertical" android:orientation="vertical" android:paddingRight="@dimen/(name removed)" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.whatsapp.TextEmojiLabel android:gravity="center_vertical" android:id="@+id/primary_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1" android:layout_toLeftOf="@+id/secondary_name" android:layout_alignParentLeft="true" style="@style/(name removed)"/>
            <com.whatsapp.TextEmojiLabel android:textAppearance="@style/(name removed)" android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/secondary_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/(name removed)" android:maxWidth="100dp" android:maxLines="1" android:layout_alignTop="@+id/primary_name" android:layout_alignBottom="@+id/primary_name" android:layout_alignParentRight="true" style="@style/(name removed)"/>
            <com.whatsapp.TextEmojiLabel android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/text_status" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1" android:layout_below="@+id/primary_name" style="@style/(name removed)"/>
        </RelativeLayout>
    </LinearLayout>
    <View android:layout_gravity="bottom" android:id="@+id/separator" style="@style/(name removed)"/>
</FrameLayout>
