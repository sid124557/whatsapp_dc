<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <solid android:color="#fff1d7"/>
            <stroke android:width="1dp" android:color="#fff1d7"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <solid android:color="#e6d6b9"/>
            <stroke android:width="1dp" android:color="#e6d6b9"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <solid android:color="#ffeecd"/>
            <stroke android:width="1dp" android:color="#ffeecd"/>
        </shape>
    </item>
</selector>
