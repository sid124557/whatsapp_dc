<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/(name removed)"/>
        </shape>
    </item>
    <item android:left="12dp" android:top="12dp" android:right="12dp" android:bottom="12dp">
        <bitmap android:gravity="center" android:src="@drawable/ic_business_advertise" android:tint="@color/(name removed)"/>
    </item>
</layer-list>
