<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true">
        <level-list>
            <item android:minLevel="0" android:maxLevel="0">
                <shape android:shape="rectangle">
                    <solid android:color="@color/(name removed)"/>
                    <corners android:radius="8dp"/>
                </shape>
            </item>
            <item android:minLevel="1" android:maxLevel="1">
                <shape android:shape="rectangle">
                    <solid android:color="@color/(name removed)"/>
                    <corners android:radius="8dp"/>
                </shape>
            </item>
        </level-list>
    </item>
</selector>
