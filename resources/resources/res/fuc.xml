    <FrameLayout android:layout_gravity="bottom" android:id="@+id/entry_container" android:paddingLeft="@dimen/(name removed)" android:layout_width="match_parent" android:layout_height="wrap_content">
        <include layout="@layout/(name removed)"/>
    </FrameLayout>
