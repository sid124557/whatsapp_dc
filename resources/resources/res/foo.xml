<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/scroll_view" android:layout_width="match_parent" android:layout_height="match_parent" android:fillViewport="true">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <com.whatsapp.WaImageView android:layout_gravity="center_horizontal" android:id="@+id/device_image" android:layout_width="88dp" android:layout_height="88dp" android:layout_marginLeft="24dp" android:layout_marginTop="16dp" android:layout_marginRight="24dp" android:layout_marginBottom="16dp" android:scaleType="fitCenter" app:NOT_FOUND_0x17="@drawable/device_list_ic_unknown_browser"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/device_name_container" android:background="?android:attr/selectableItemBackground" android:paddingLeft="24dp" android:paddingTop="16dp" android:paddingRight="24dp" android:paddingBottom="16dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <com.whatsapp.WaTextView android:textSize="14sp" android:textColor="@color/(name removed)" android:id="@+id/device_name_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/(name removed)" android:lineSpacingExtra="3sp"/>
                <com.whatsapp.WaTextView android:textSize="16sp" android:textColor="?attr/(name removed)" android:id="@+id/device_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:lineSpacingExtra="2sp"/>
            </LinearLayout>
            <com.whatsapp.WaImageView android:layout_gravity="center_vertical" android:id="@+id/device_name_edit_btn" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginStart="4dp" app:NOT_FOUND_0x17="@drawable/ic_action_edit" app:NOT_FOUND_0x18="@color/(name removed)"/>
        </LinearLayout>
        <View style="@style/(name removed)"/>
        <com.whatsapp.WaTextView android:textSize="14sp" android:textColor="@color/(name removed)" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="24dp" android:layout_marginTop="24dp" android:layout_marginRight="24dp" android:layout_marginBottom="8dp" android:text="@string/(name removed)"/>
        <LinearLayout android:id="@+id/status_container" style="@style/(name removed)">
            <com.whatsapp.WaImageView android:id="@+id/status_icon" app:NOT_FOUND_0x17="@drawable/ic_business_hours" style="@style/(name removed)"/>
            <com.whatsapp.WaTextView android:id="@+id/status_text" style="@style/(name removed)"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/platform_container" style="@style/(name removed)">
            <com.whatsapp.WaImageView android:id="@+id/platform_icon" app:NOT_FOUND_0x17="@drawable/ic_settings_roaming" style="@style/(name removed)"/>
            <com.whatsapp.WaTextView android:id="@+id/platform_text" style="@style/(name removed)"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/location_container" style="@style/(name removed)">
            <com.whatsapp.WaImageView android:id="@+id/location_icon" app:NOT_FOUND_0x17="@drawable/ic_current_location" style="@style/(name removed)"/>
            <com.whatsapp.WaTextView android:id="@+id/location_text" style="@style/(name removed)"/>
        </LinearLayout>
        <com.whatsapp.wds.components.button.WDSButton android:id="@+id/log_out_btn" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="16dp" android:text="@string/(name removed)" style="@style/(name removed)"/>
    </LinearLayout>
</ScrollView>
