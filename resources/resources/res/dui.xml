<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/(name removed)">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/(name removed)"/>
            <corners android:radius="16dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:right="1dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="?attr/(name removed)"/>
            <corners android:radius="16dp"/>
        </shape>
    </item>
</ripple>
