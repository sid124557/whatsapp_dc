<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <size android:height="@dimen/(name removed)"/>
            <solid android:color="?attr/(name removed)"/>
        </shape>
    </item>
    <item android:left="0dp" android:top="0dp" android:right="0dp" android:bottom="6dp">
        <shape>
            <gradient android:startColor="@color/(name removed)" android:endColor="@color/(name removed)" android:angle="90" android:type="linear"/>
        </shape>
    </item>
</layer-list>
