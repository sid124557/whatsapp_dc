<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/(name removed)">
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="@dimen/(name removed)" android:color="@color/(name removed)"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <inset android:insetLeft="1dp" android:insetRight="1dp" android:insetTop="1dp" android:insetBottom="1dp">
            <shape android:shape="oval">
                <solid android:color="@android:color/white"/>
            </shape>
        </inset>
    </item>
</ripple>
