<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/(name removed)"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/(name removed)"/>
        </shape>
    </item>
    <item android:state_enabled="true" android:drawable="@color/(name removed)"/>
</selector>
