<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:left="-8dp" android:top="-8dp" android:right="-8dp" android:bottom="-8dp">
        <shape>
            <stroke android:width="8dp" android:color="?attr/(name removed)"/>
            <corners android:radius="16dp"/>
            <solid android:color="@color/(name removed)"/>
        </shape>
    </item>
</layer-list>
