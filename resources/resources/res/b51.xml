<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/(name removed)">
    <item android:id="@android:id/mask" android:drawable="@color/(name removed)"/>
    <item>
        <selector>
            <item android:drawable="@color/(name removed)" android:state_activated="true"/>
            <item android:state_pressed="true" android:drawable="@color/(name removed)"/>
            <item android:drawable="@color/(name removed)" android:state_activated="false"/>
        </selector>
    </item>
</ripple>
